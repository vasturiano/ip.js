{
  "name": "ip.js",
  "version": "1.3.11",
  "description": "A JS library for manipulating IP addresses, prefixes and ranges. Supports both IPv4 and IPv6.",
  "unpkg": "dist/ip.min.js",
  "jsdelivr": "dist/ip.min.js",
  "main": "dist/ip.common.js",
  "module": "dist/ip.module.js",
  "types": "dist/ip.d.ts",
  "sideEffects": false,
  "keywords": [
    "ip",
    "prefix",
    "range",
    "ipv4",
    "ipv6",
    "internet",
    "protocol",
    "network",
    "bgp",
    "tcp/ip"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "license": "MIT",
  "homepage": "https://github.com/vasturiano/ip.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/ip.js.git"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/ip.js/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -w -c rollup.config.dev.mjs",
    "test": "jest",
    "prepare": "npm run build"
  },
  "dependencies": {
    "jsbi": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.36.0",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.8.2"
  }
}
